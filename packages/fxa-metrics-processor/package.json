    "@types/sinon": "^9.0.0",
    "audit-filter": "^0.5.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "json-schema-to-typescript": "^8.2.0",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.4",
    "sinon": "^9.0.2",
    "ts-node": "8.8.2",
    "ts-sinon": "^1.0.25",
    "tslint": "6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.8.3",
    "uuid": "^7.0.3"
