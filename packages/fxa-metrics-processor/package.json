  },
  "bugs": {
    "url": "https://github.com/mozilla/fxa/issues"
  },
  "homepage": "https://github.com/mozilla/fxa#readme",
  "readmeFilename": "README.md",
  "dependencies": {
    "@google-cloud/pubsub": "^1.7.2",
    "@sentry/integrations": "^5.15.4",
    "@sentry/node": "^5.15.4",
    "@types/convict": "^4.2.1",
    "ajv": "^6.12.0",
    "convict": "^5.2.0",
    "mozlog": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
