{
  "extends": [
    "plugin:fxa/server",
    "plugin:jest/recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["fxa"],
  "ignorePatterns": ["build/"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "off",
    "strict": "off",
    "no-useless-escape": "off",
    "handle-callback-err": "off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
